class Solution {
    public String solution(int num) {
        String answer = "";
        int nums = 0;
        if(num<0){
            nums = num * (-1);
        }else{
            nums = num;
        }
        if(nums%2==1){
            answer = "Odd";
        }else {
            answer = "Even";
        }
        
        return answer;
    }
}
